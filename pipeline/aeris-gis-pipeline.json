{
	"name": "aeris-gis-pipeline",
	"properties": {
		"description": "Pipeline for importing aeris weather data",
		"activities": [
			{
				"name": "CopyTrafficData",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "XmlSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "XmlReadSettings",
							"validationMode": "none"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['TRAFFIC_ITEM_ID']"
								},
								"sink": {
									"path": "TRAFFIC_ITEM_ID1"
								}
							},
							{
								"source": {
									"path": "['TRAFFIC_ITEM_STATUS_SHORT_DESC']"
								},
								"sink": {
									"path": "TRAFFIC_STATUS_SHORT_DESC1"
								}
							},
							{
								"source": {
									"path": "['TRAFFIC_ITEM_TYPE_DESC']"
								},
								"sink": {
									"path": "TRAFFIC_ITEM_TYPE_DESC1"
								}
							},
							{
								"source": {
									"path": "['START_TIME']"
								},
								"sink": {
									"path": "START_TIME1"
								}
							},
							{
								"source": {
									"path": "['END_TIME']"
								},
								"sink": {
									"path": "END_TIME1"
								}
							},
							{
								"source": {
									"path": "['ENTRY_TIME']"
								},
								"sink": {
									"path": "ENTRY_TIME1"
								}
							},
							{
								"source": {
									"path": "['LOCATION']['INTERSECTION']['ORIGIN']['STREET1']['ADDRESS1']"
								},
								"sink": {
									"path": "ADDRESS1"
								}
							},
							{
								"source": {
									"path": "['LOCATION']['INTERSECTION']['ORIGIN']['STREET2']['ADDRESS1']"
								},
								"sink": {
									"path": "ADDRESS2"
								}
							},
							{
								"source": {
									"path": "['LOCATION']['INTERSECTION']['ORIGIN']['COUNTY']"
								},
								"sink": {
									"path": "COUNTY"
								}
							},
							{
								"source": {
									"path": "['LOCATION']['INTERSECTION']['ORIGIN']['STATE']"
								},
								"sink": {
									"path": "STATE"
								}
							},
							{
								"source": {
									"path": "['LOCATION']['GEOLOC']['ORIGIN']['LATITUDE']"
								},
								"sink": {
									"path": "LAT"
								}
							},
							{
								"source": {
									"path": "['LOCATION']['GEOLOC']['ORIGIN']['LONGITUDE']"
								},
								"sink": {
									"path": "LONG"
								}
							}
						],
						"collectionReference": "$['TRAFFICML_INCIDENTS']['TRAFFIC_ITEMS']['TRAFFIC_ITEM']"
					}
				},
				"inputs": [
					{
						"referenceName": "Xml2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}